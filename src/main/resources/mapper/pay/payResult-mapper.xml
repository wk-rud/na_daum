<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="payResult">
	
	<select id="selectOnePayResult" resultType="payResult">
		select * from member where partnerOrderId = #{partnerOrderId}
	</select>
	
	<insert id="insertPayResult"></insert>
	
	
	
	<select id="selectOnePayMemberInfo" resultType="member">
		select * from member where id = #{id}
	</select>
	
</mapper>

